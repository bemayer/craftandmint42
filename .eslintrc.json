{
  "extends": [
    "airbnb",
    "airbnb-typescript/base",
    "prettier",
    "plugin:node/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": ["@typescript-eslint", "import", "prettier"],
  "rules": {
    "no-undef": "off",
    "no-console": "off",
    "no-use-before": "off"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}
